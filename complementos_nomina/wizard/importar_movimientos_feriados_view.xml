<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="importar_movimientos_feriados_form_view" model="ir.ui.view">
        <field name="name">importar.movimientos.feriados.form.view</field>
        <field name="model">importar.movimientos.feriados</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="archivo"/>
                    </group>
                </group>
                <footer>
                    <button string='Importar' name="importar_archivo" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>