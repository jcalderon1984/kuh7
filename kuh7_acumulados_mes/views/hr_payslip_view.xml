<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="hr_payslip_form_info_aditional_kuh7" model="ir.ui.view">
        <field name="name">hr.payslip.form.info.additional.kuh7</field>
        <field name="model">hr.payslip</field>
        <field name="inherit_id" ref="nomina_cfdi_ee.view_hr_payslip_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='fondo_ahorro']" position="after">
                <group name="fondo_ahorro" string="Fondo de ahorro">
                    <field name="accumulated_taxable_perceptions"/>
                    <field name="accumulated_isr_sp"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
