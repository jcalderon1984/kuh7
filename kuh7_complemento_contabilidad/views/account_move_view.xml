<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="account_move_view_account_date_kuh7" model="ir.ui.view">
        <field name="name">account.move.modify.view.kuh7</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@id='header_right_group']/field[@name='date']" position="replace">
                <field name="date"  string="Fecha Contable"/>
            </xpath>
        </field>
    </record>
</odoo>